<f:view xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:pm="http://primefaces.org/mobile" contentType="text/html">
	
	<ui:composition template="../templates/comumExterno.xhtml">

		<ui:define name="pageCenter">
			
			
            <h:form id="frm">
                <h:outputText value="Button Group"/>
                <pm:buttonGroup>
                    <p:commandButton value="Accept" action="#{testeMB.habilitaAcept}" update="frm"/>
                    <p:commandButton value="Deny" action="#{testeMB.desabilitaAcept}" update="frm"/>
                </pm:buttonGroup>
                <p:separator/>
                <h:outputText value="Input Range"/>
                <pm:inputRange value="23"/>
                <p:separator/>
                <h:outputText value="Nav Bar"/>
                <pm:content>
                    <pm:navBar>
                        <p:button value="Home" icon="home" href="#components" styleClass="ui-btn-active"/>
                        <p:button value="Settings" icon="gear" href="#components"/>
                        <p:button value="About" icon="info" href="#components"/>
                    </pm:navBar>
                </pm:content>
                <p:separator/>
                <h:outputText value="Switch"/>
                <pm:switch value="#{testeMB.acept}" onLabel="Accept" offLabel="deny"/>
                
                
            </h:form>
            


		</ui:define>
		
	</ui:composition>

</f:view>