.ui-state-hover, .ui-widget-content .ui-state-hover, .ui-widget-header .ui-state-hover, 
.ui-state-focus, .ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus {
	background: #1EAEE3;
	color: #FFF;
}

.ui-widget-header {
	border: 1px solid #1EAEE3;
	background: #1EAEE3;
	color: #eaf5f7;
	font-weight: bold;
}


.ui-wizard {
	margin: 0;
	padding: 15px;
}

.ui-datatable, .ui-datatable-header, .ui-widget-header, .ui-corner-top {
	margin: 0;
	padding: 15px;
	border: 1px solid #1EAEE3;
	background: #1EAEE3;
	font-weight: normal;
	color: #ffffff;
	font-size: 18px;
}

.ui-datatable table {
	background: #1EAEE3;
	font-weight: normal;
	color: #ffffff;
	font-size: 18px;
}


.ui-editable-column ui-state-highlight ui-cell-editing{
	border: 1px solid #333;
	background: #ffffff;
}
.ui-cell-editor-input{
	border: 1px solid #333;
	background: #ffffff;
}

.ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default {
	border: 1px solid #1EAEE3;
	background: #1EAEE3;
	font-weight: normal;
	color: #ffffff;
}

.ui-state-highlight {
	border: 1px solid #ED6521;
	background: #ED6521;	
	font-weight: normal;
	color: #ffffff;
}



button.ui-state-default 	{
	border: 1px solid #ED6521;
	background: #ED6521;	
	font-weight: normal;
	color: #ffffff;
	
}

span.ui-button-text {
	font-size: 18px;
}

.ui-button-icon-only .ui-icon, .ui-button-text-icon-left .ui-icon, .ui-button-text-icon-right .ui-icon{
	font-size: 25px;
}

.ui-state-focus .ui-icon {
 background-image: #ffffff; 
}


