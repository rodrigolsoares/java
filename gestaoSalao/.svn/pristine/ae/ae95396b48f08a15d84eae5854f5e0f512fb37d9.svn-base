<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">

	<h1><p:outputLabel value="Dados Responsável" /></h1>	
	<p:separator/><br />
	
	<h:panelGrid columns="5" cellpadding="10"> 
		
		<h:panelGroup> 
			<p:outputLabel value="#{msg.nomeResp}" /><br />	
			<h:outputText  value ="#{salaoMB.responsavel.nome}"   />
		</h:panelGroup>
	
		<h:panelGroup> 
			<p:outputLabel value="#{msg.sexo} " /> <br />	
			<h:outputText value="#{salaoMB.responsavel.sexo}" />  
	       
	    </h:panelGroup> 
	    
	    <h:panelGroup>
			<p:outputLabel value="#{msg.rg} " /><br />	
			<h:outputText  value = "#{salaoMB.responsavel.rg}"  />
		</h:panelGroup>
		
		<h:panelGroup>
			<p:outputLabel value="#{msg.cpf} " /><br />	
			<h:outputText  value = "#{salaoMB.responsavel.cpf}" />
		</h:panelGroup>
		
		<h:panelGroup>
			<p:outputLabel value="#{msg.dtNasc} " /><br />	
			<h:outputText value = "#{salaoMB.responsavel.dtNascimento}" >
				<f:convertDateTime pattern="dd/MM/yyyy" />	
			</h:outputText>
		</h:panelGroup>
		
		
      
	</h:panelGrid>
		
	<br />
	<h3><p:outputLabel value="Contato Responsável" /></h3>
	<p:separator/><br />

	<h:panelGrid columns="4" cellpadding="5">
		 
		<h:panelGroup>
			<p:outputLabel value="#{msg.tel_residencial} " /><br />	
			<h:outputText value = "#{salaoMB.responsavel.telResidencial}" />
		</h:panelGroup>
		
		<h:panelGroup>
			<p:outputLabel value="#{msg.tel_celular} " /><br />	
			<h:outputText value = "#{salaoMB.responsavel.telCelular}" />
		</h:panelGroup>
		
		<h:panelGroup>
			<p:outputLabel value="#{msg.tel_comercial} " /><br />	
			<h:outputText value = "#{salaoMB.responsavel.telComercial}" />
		</h:panelGroup>
		
		<h:panelGroup>
			<p:outputLabel value="#{msg.email} " /><br />	
			<h:outputText value = "#{salaoMB.responsavel.email}" />
		</h:panelGroup>
		
		
	</h:panelGrid>

</html>
