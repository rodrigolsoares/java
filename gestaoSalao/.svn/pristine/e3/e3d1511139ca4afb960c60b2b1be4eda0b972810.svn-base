package br.com.gestao.salao.mobile.Controller;

import java.io.Serializable;

import javax.annotation.PostConstruct;

import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Controller;

@Controller
@Scope("session")
public class TesteMB implements Serializable{
	
	private boolean acept = false;
	
	@PostConstruct
	public void init(){
		
		
	}
	
	public void habilitaAcept(){
		acept = true;
	}
	
	public void desabilitaAcept(){
		acept = false;
	}

	public boolean isAcept() {
		return acept;
	}

	public void setAcept(boolean acept) {
		this.acept = acept;
	}
	
	
	
	
	

}
